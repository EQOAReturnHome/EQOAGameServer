<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
  <appSettings>
    <add key="UseDecryption" value="false"/>
    <add key="DESkey" value=""/>
  </appSettings>
  <connectionStrings>
    <clear />
    <add name="DevLocal"
     providerName="System.Data.ProviderName"
     connectionString="server=$(DB_IP_ADDRESS);userid=$(DB_USER);password=$(DB_PASSWORD);database=$(DB_NAME);Allow User Variables=True" />
  </connectionStrings>
</configuration>
