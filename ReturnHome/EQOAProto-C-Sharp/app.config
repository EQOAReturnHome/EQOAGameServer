<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
  <appSettings>
    <add key="LocalServerPort" value="10070"/>
    <add key="StaticAccount" value="2"/>

    <add key="Servercount" value="2"/>

    <add key="Server0" value="Castle Lightwolf"/>
    <add key="ServerIP0" value="$(IP_ADDRESS_1)"/>
    <add key="ServerPort0" value="10070"/>
    <add key="ServerRecommended0" value="1"/>
    <add key="ServerEndPointID0" value="29617"/>
    <add key="ServerLanguage0" value="0"/>

    <add key="Server1" value="Hodstock"/>
    <add key="ServerIP1" value="$(IP_ADDRESS_2)"/>
    <add key="ServerPort1" value="10071"/>
    <add key="ServerRecommended1" value="0"/>
    <add key="ServerEndPointID1" value="29616"/>
    <add key="ServerLanguage1" value="0"/>

    <add key="UseDecryption" value="false"/>
    <add key="AESkey" value=""/>
  </appSettings>
  <connectionStrings>
    <clear />
    <add name="DevLocal"
     providerName="System.Data.ProviderName"
     connectionString="server=$(DB_IP_ADDRESS);userid=$(DB_USER);password=$(DB_PASSWORD);database=$(DB_NAME);Allow User Variables=True" />
  </connectionStrings>
</configuration>
